{
    "manifest_version": 3,
    "name": "DevTools Request Resender",
    "description": "An extension to capture, modify, and resend network requests from a DevTools panel.",
    "version": "1.0",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "devtools"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "devtools_page": "devtools.html"
}